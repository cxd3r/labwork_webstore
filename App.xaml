<Application
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	x:Class="WpfPrototype.App" mc:Ignorable="d">
	<Application.Resources>
		<!-- Resources scoped at the Application level should be defined here. -->
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Microsoft.Expression.Prototyping.SketchControls;Component/ScrollViewerStyles.xaml"/>
				<ResourceDictionary Source="/WpfPrototype.Screens;Component/SketchStyles.xaml"/>
				<ResourceDictionary Source="/WpfPrototype.Screens;Component/Simple Styles.xaml"/>
			</ResourceDictionary.MergedDictionaries>
			<ControlTemplate x:Key="CommonStyles_Button" TargetType="{x:Type Button}">
				<Grid Cursor="Hand" Background="White"/>
			</ControlTemplate>
			<DataTemplate x:Key="CommonStyles_DataTemplate">
				<Grid/>
			</DataTemplate>
		</ResourceDictionary>
	</Application.Resources>
</Application>