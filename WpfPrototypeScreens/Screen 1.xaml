				<i:EventTrigger EventName="Click">
					<pb:NavigateToScreenAction TargetScreen="WpfPrototypeScreens.Screen_1_2"/>
				</i:EventTrigger>
			</i:Interaction.Triggers>
		</Button>
		<Image x:Name="___img__product_block_3" Margin="415,0,440,85" VerticalAlignment="Bottom" Width="225" Height="200.004" Source="t-shirt_1.jpg" d:LayoutOverrides="VerticalAlignment" Stretch="UniformToFill"/>
		<Rectangle x:Name="product_block_4" Fill="White" Stroke="Black" StrokeThickness="0" HorizontalAlignment="Left" Margin="685,0,0,50" Width="225" Height="250.004" VerticalAlignment="Bottom"/>
		<Button x:Name="___button__more_product_block_4" Cursor="Hand" HorizontalAlignment="Left" Margin="685,0,0,50" Style="{DynamicResource SimpleButton}" VerticalAlignment="Bottom" Width="225" Height="25" Content="more" Background="Black" BorderBrush="{x:Null}" BorderThickness="0" FontFamily="Courier New" FontSize="10.667" Foreground="#BFFFFFFF">
			<i:Interaction.Triggers>
				<i:EventTrigger EventName="Click">
					<pb:NavigateToScreenAction TargetScreen="WpfPrototypeScreens.Screen_1_2"/>
				</i:EventTrigger>
			</i:Interaction.Triggers>
		</Button>
		<Image x:Name="___img__product_block_4" Margin="0,0,170,85" VerticalAlignment="Bottom" Width="225" Height="200.004" Source="t-shirt_2.jpg" d:LayoutOverrides="VerticalAlignment, Width" HorizontalAlignment="Right" Stretch="UniformToFill"/>
	</Grid>
</UserControl>