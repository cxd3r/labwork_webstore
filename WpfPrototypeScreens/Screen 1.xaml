			<ListBoxItem Content="subfilter_3" FontFamily="Courier New" FontSize="9.333" Foreground="#FFFAB4B4"/>
		</ComboBox>
		<TextBlock HorizontalAlignment="Left" Margin="100,505,0,0" VerticalAlignment="Top" FontFamily="Courier New" Text="filter 6" TextWrapping="Wrap" Opacity="0.5" FontStyle="Italic"/>
