		</ComboBox>
		<TextBlock HorizontalAlignment="Left" Margin="100,565,0,0" FontFamily="Courier New" Text="filter 7" TextWrapping="Wrap" Opacity="0.5" FontStyle="Italic" VerticalAlignment="Top"/>
		<ComboBox Background="White" HorizontalAlignment="Left" Margin="100,585,0,0" VerticalAlignment="Top" Width="150" Height="25" BorderBrush="{x:Null}" FontFamily="Courier New">
			<ListBoxItem Content="subfilter_1" FontFamily="Courier New" FontSize="9.333" Foreground="#FFFAB4B4"/>
			<ListBoxItem Content="subfilter_2" FontFamily="Courier New" FontSize="9.333" Foreground="#FFFAB4B4"/>
			<ListBoxItem Content="subfilter_3" FontFamily="Courier New" FontSize="9.333" Foreground="#FFFAB4B4"/>
		</ComboBox>
		<Rectangle x:Name="product_block_1" Fill="White" Stroke="Black" StrokeThickness="0" Margin="415,160,0,0" Width="225" Height="250" HorizontalAlignment="Left" VerticalAlignment="Top"/>
		<Button x:Name="___button__more_product_block_1" HorizontalAlignment="Left" Margin="415,0,0,310" Width="225" Content="more" Style="{DynamicResource SimpleButton}" Background="Black" BorderBrush="{x:Null}" Foreground="#BFFFFFFF" Height="25" BorderThickness="0" FontFamily="Courier New" FontSize="10.667" VerticalAlignment="Bottom" Cursor="Hand">
			<i:Interaction.Triggers>
				<i:EventTrigger EventName="Click">
					<pb:NavigateToScreenAction TargetScreen="WpfPrototypeScreens.Screen_1_2"/>
				</i:EventTrigger>
			</i:Interaction.Triggers>
		</Button>
		<Image x:Name="___img__product_block_1" Margin="415,170,440,0" VerticalAlignment="Top" Height="200" Source="hoodie_1.jpg" Width="225"/>
		<Rectangle x:Name="product_block_2" Fill="White" Stroke="Black" StrokeThickness="0" HorizontalAlignment="Left" Margin="685,160,0,310" Width="225" Height="250"/>
